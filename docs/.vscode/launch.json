    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--frontend",
                "lean4+markup",
                "basics.lean",
                "--backend",
                "webpage",
                "--lake",
                "lakefile.lean",
                "-o",
                "basics.lean.md",
            ]
        }
    ]
}